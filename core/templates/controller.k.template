helpers = require :helpers
ApplicationController = helpers.get_controller :application

class %controllerClassName% extends ApplicationController {
  @%modelName% = helpers.get_model("%modelName%")

  @initialize = -> {
    @name = "%modelName%"
    super()
  }

  @index = -> {
    @render @index.view
  }
}

module.exports = new %controllerClassName%()

